{
    "name": "gulch/doctrine-test",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Volodymyr Gulchuk",
            "email": "volodymyr@gulchuk.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/orm": "^2.5",
        "monolog/monolog": "^1.23",
        "symfony/finder": "^3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.7",
        "robmorgan/phinx": "^0.9.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "exclude-from-classmap": [
            "/Tests/",
            "/test/",
            "/tests/"
        ],
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
